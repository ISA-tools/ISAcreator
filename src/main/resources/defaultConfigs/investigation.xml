<isatab-config-file xmlns="http://www.ebi.ac.uk/bii/isatab_configuration#">
    <isatab-configuration table-name="investigation">
        <measurement term-label="[investigation]" term-accession="" source-abbreviation=""/>
        <technology term-label="" term-accession="" source-abbreviation=""/>

        <!--Investigation Section -->

        <field header="Investigation Identifier" section="INVESTIGATION" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>A user defined identifier for the investigation.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Title" section="INVESTIGATION" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>A title for the Investigation.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Description" section="INVESTIGATION" data-type="Long String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>A brief description of the investigation aims.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Submission Date" section="INVESTIGATION" data-type="Date" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The date the investigation was submitted.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Public Release Date" section="INVESTIGATION" data-type="Date" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The date the investigation was release publicly.
            </description>
            <default-value></default-value>
        </field>

        <field header="Comment [Created with configuration]" is-hidden="true" section="INVESTIGATION" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>A field indicating which ISA configuration was used to create the ISAtab file.
            </description>
            <default-value></default-value>
        </field>

        <field header="Comment [Last Opened With Configuration]" is-hidden="true" section="INVESTIGATION" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>A field indicating which ISA configuration was last used to open the ISAtab file.
            </description>
            <default-value></default-value>
        </field>

        <!--Investigation Publications Section -->

        <field header="Investigation PubMed ID" section="INVESTIGATION PUBLICATIONS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The PubMed ID related to this publication (if available).
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Publication DOI" section="INVESTIGATION PUBLICATIONS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The DOI related to this publication (if available).
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Publication Author List" section="INVESTIGATION PUBLICATIONS"
               data-type="Long String" is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The authors who contributed to this publication.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Publication Title" section="INVESTIGATION PUBLICATIONS" data-type="Long String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The title of the publication.
            </description>
            <default-value></default-value>
        </field>


        <field header="Investigation Publication Status" section="INVESTIGATION PUBLICATIONS" data-type="Ontology term"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The status of this publication, e.g. Published, Submitted, etc.
            </description>
            <default-value></default-value>
        </field>


        <!--Investigation Contacts Section -->

        <field header="Investigation Person Last Name" section="INVESTIGATION CONTACTS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The last name/surname of the contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Person First Name" section="INVESTIGATION CONTACTS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The first name/forename of the contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Person Mid Initials" section="INVESTIGATION CONTACTS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The mid initial of the contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Person Email" section="INVESTIGATION CONTACTS" data-type="Long String"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The email corresponding to this contact.
            </description>
            <default-value></default-value>
        </field>


        <field header="Investigation Person Phone" section="INVESTIGATION CONTACTS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>A phone number for this contact, if available.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Person Fax" section="INVESTIGATION CONTACTS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>A fax number for this contact, if available.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Person Address" section="INVESTIGATION CONTACTS" data-type="Long String"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The address for this contact, preferably, their institution.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Person Affiliation" section="INVESTIGATION CONTACTS" data-type="String"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The affiliation for this contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Investigation Person Roles" section="INVESTIGATION CONTACTS" data-type="Ontology term"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The role of the person in this investigation.
            </description>
            <default-value></default-value>
        </field>


        <!--Study Section -->

        <field header="Study Identifier" section="STUDY" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>A user defined identifier for the study.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Title" section="STUDY" data-type="String" is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>A title for the Study.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Description" section="STUDY" data-type="Long String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>A brief description of the study aims.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Submission Date" section="STUDY" data-type="Date" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The date the study was submitted.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Public Release Date" section="STUDY" data-type="Date" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The date the study was released publicly.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study File Name" is-hidden="true" section="STUDY" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The name of the study sample file linked to this Study
            </description>
            <default-value></default-value>
        </field>


        <!-- Study Publications Section -->

        <field header="Study PubMed ID" section="STUDY PUBLICATIONS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>The PubMed ID related to this publication (if available).
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Publication DOI" section="STUDY PUBLICATIONS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>The DOI related to this publication (if available).
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Publication Author List" section="STUDY PUBLICATIONS" data-type="Long String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The authors who contributed to this publication.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Publication Title" section="STUDY PUBLICATIONS" data-type="Long String"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The title of the publication.
            </description>
            <default-value></default-value>
        </field>


        <field header="Study Publication Status" section="STUDY PUBLICATIONS" data-type="Ontology term"
               is-file-field="false" is-multiple-value="false"
               is-required="true">
            <description>The status of this publication, e.g. Published, Submitted, etc.
            </description>
            <default-value></default-value>
        </field>


        <!--Study Contacts Section -->

        <field header="Study Person Last Name" section="STUDY CONTACTS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>The last name/surname of the contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Person First Name" section="STUDY CONTACTS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>The first name/forename of the contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Person Mid Initials" section="STUDY CONTACTS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="true">
            <description>The mid initial of the contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Person Email" section="STUDY CONTACTS" data-type="Long String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The email corresponding to this contact.
            </description>
            <default-value></default-value>
        </field>


        <field header="Study Person Phone" section="STUDY CONTACTS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>A phone number for this contact, if available.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Person Fax" section="STUDY CONTACTS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>A fax number for this contact, if available.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Person Address" section="STUDY CONTACTS" data-type="Long String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The address for this contact, preferably, their institution.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Person Affiliation" section="STUDY CONTACTS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The affiliation for this contact.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Person Roles" section="STUDY CONTACTS" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The role of the person in this Study.
            </description>
            <default-value></default-value>
        </field>

        <!--Study Factors Section -->

        <field header="Study Factor Name" section="STUDY FACTORS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The name of the Factor driving this study, e.g. Dose, Collection Time Point, Compound, and so forth
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Factor Type" section="STUDY FACTORS" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The type of the Factor driving this study, e.g. EFO:Dose (from Ontology), or Dose if the Factor name is something related to Dose as well, etc.
            </description>
            <default-value></default-value>
        </field>

        <!--Study Designs Section -->

        <field header="Study Design Type" section="STUDY DESIGN DESCRIPTORS" data-type="Ontology term"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The type of study design being employed, e.g. Observation, Intervention, Cross-Over, etc.
            </description>
            <default-value></default-value>
        </field>

        <!--Study Assays Section -->

        <field header="Study Assay Measurement Type" section="STUDY ASSAYS" data-type="List" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The measurement being observed in this assay, e.g. Transcription Profiling, Metabolite Profiling
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Assay Technology Type" section="STUDY ASSAYS" data-type="List" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The technology being employed to observe this measurement, e.g. DNA microarray, NMR, Mass Spectrometry and so forth
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Assay Technology Platform" section="STUDY ASSAYS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The technology platform used, e.g. Affymetrix, Illumina, Solexa, SOLiD, Bruker, etc.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Assay File Name" section="STUDY ASSAYS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The name of the file which contains the information about this assay, the samples and pipeline used.
            </description>
            <default-value></default-value>
        </field>

        <!--Study Assays Section -->

        <field header="Study Protocol Name" section="STUDY PROTOCOLS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The name of the protocol, e.g. RNA Extraction Protocol
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Protocol Type" section="STUDY PROTOCOLS" data-type="Ontology term" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The type of the protocol, preferably coming from an Ontology, e.g. Extraction Protocol
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Protocol Description" section="STUDY PROTOCOLS" data-type="Long String"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>A description of the Protocol.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Protocol URI" section="STUDY PROTOCOLS" data-type="Long String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>A URI to link out to a publication, web page, etc. describing the protocol.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Protocol Version" section="STUDY PROTOCOLS" data-type="String" is-file-field="false"
               is-multiple-value="false"
               is-required="false">
            <description>The version of the protocol used, where applicable.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Protocol Parameters Name" section="STUDY PROTOCOLS" data-type="Ontology term"
               is-file-field="false" is-multiple-value="true"
               is-required="false">
            <description>The parameters used when executing this protocol.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Protocol Components Name" section="STUDY PROTOCOLS" data-type="String"
               is-file-field="false" is-multiple-value="true"
               is-required="false">
            <description>The components used when carrying out this protocol.
            </description>
            <default-value></default-value>
        </field>

        <field header="Study Protocol Components Type" section="STUDY PROTOCOLS" data-type="Ontology term"
               is-file-field="false" is-multiple-value="false"
               is-required="false">
            <description>The type of Components used in this Protocol.
            </description>
            <default-value></default-value>
        </field>

    </isatab-configuration>
</isatab-config-file>